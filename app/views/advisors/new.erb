<section>
<h1>Advisor sign up</h1>
</section>

<div>
 	<%= form_for(:advisor) do |f| %>
		<%= f.label :email %>
		<%= f.text_field :email %> <br><br>

		<%= f.label :"first name" %>
		<%= f.text_field :firstname %> <br><br>

		<%= f.label :"last name" %>
		<%= f.text_field :lastname %> <br><br>		

		<%= f.label :"industry" %>
		<%= f.text_field :field %> <br><br>		

		<%= f.label :university %>
		<%= f.text_field :university %> <br><br>

	  <%= f.submit "Sign me up" %>
	<% end %>